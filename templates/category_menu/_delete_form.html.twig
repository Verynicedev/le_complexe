<form method="post" action="{{ path('category_menu_delete', {'id': category_menu.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category_menu.id) }}">
    <button class="btn btn-primary text-white">Supprimer</button>
</form>
